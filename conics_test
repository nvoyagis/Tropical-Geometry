        info(D2)
        info(D3)
        info(E1)
        info(E2)
        info(E3)
        info(F1)
        info(F2) # Wrong
        info(F3) # Wrong
        info(G1)

    def test_A1(self) -> None:
        info([0.1, 0, 3.1, -0.3, 1.4, 1])






