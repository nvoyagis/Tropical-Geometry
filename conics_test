class Test_conics(unittest.TestCase):



    def test_basic_conics(self) -> None:
        info(A1)
        info(B1)
        info(B2)
        info(B3)
        info(C1)
        info(C2)
        info(C3)
        info(C4)
        info(C5)
        info(C6)
